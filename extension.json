{
  "name": "MeilisearchForMediaWiki",
  "author": ["Lex Sulzer"],
  "url": "https://www.mediawiki.org/wiki/Extension:MeilisearchForMediaWiki",
  "license-name": "GPL-2.0-or-later",
  "descriptionmsg": "meilisearch-desc",
  "type": "other",
  "requires": {
    "MediaWiki": ">= 1.35.0"
  },
  "AutoloadNamespaces": {
    "MediaWiki\\Extension\\MeilisearchForMediaWiki\\": "src/"
  },
  "SpecialPages": {
    "Meilisearch": "SpecialMeilisearch"
  },
  "config": {
    "BoilerPlateEnableFoo": {
      "description": "Enable the Foo feature.",
      "value": true
    },
    "BoilerPlateVandalizeEachPage": {
      "description": "Write 'MeilisearchForMediaWiki was here' on each page",
      "value": false
    }
  },
  "HookHandlers": {
    "MeilisearchForMediaWikiHooks": {
      "class": "MediaWiki\\Extension\\MeilisearchForMediaWiki\\Hooks"
    }
  },
  "AutoloadClasses": {
    "SpecialMeilisearch": "includes/specials/SpecialMeilisearch.php"
  },
  "Hooks": {
    "BeforePageDisplay": "MeilisearchForMediaWikiHooks"
  },
  "MessagesDirs": {
    "MeilisearchForMediaWiki": ["i18n"]
  },
  "ResourceModules": {
    "ext.meilisearchForMediaWiki": {
      "localBasePath": "resources/ext.meilisearchForMediaWiki",
      "remoteExtPath": "MeilisearchForMediaWiki/resources/ext.meilisearchForMediaWiki",
      "dependencies": [],
      "styles": [],
      "packageFiles": [
        "instant-meilisearch.umd.min.js",
        "instantsearch.js@4",
        "index.js",
        "init.js"
      ],
      "messages": []
    }
  },
  "manifest_version": 2
}
